{
  "name": "@nettoolkit/prerender-loader",
  "version": "1.0.0",
  "description": "Painless universal prerendering for Webpack 5. Works great with html-webpack-plugin.",
  "main": "src/index.js",
  "source": "src/index.js",
  "license": "Apache-2.0",
  "author": "The Chromium Authors, NetToolKit",
  "contributors": [
    {
      "name": "Daniel Woznicki",
      "email": "danwoz@nettoolkit.com"
    }
  ],
  "keywords": [
    "pre-render",
    "prerendering",
    "webpack plugin",
    "webpack 5",
    "SSR",
    "performance",
    "first contentful paint",
    "FCP"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nettoolkit/prerender-loader.git"
  },
  "bugs": {
    "url": "https://github.com/nettoolkit/prerender-loader/issues"
  },
  "homepage": "https://github.com/nettoolkit/prerender-loader#readme",
  "scripts": {
    "docs": "documentation readme -q --no-markdown-toc -a public -s Usage --sort-order alpha src",
    "test": "jest --coverage"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*"
    ],
    "watchPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "plugin:jest/recommended"
    ],
    "env": {
      "browser": false,
      "node": true
    },
    "rules": {
      "indent": [
        2,
        2
      ],
      "semi": [
        2,
        "always"
      ],
      "prefer-const": 1
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.0",
    "babel-preset-env": "^1.7.0",
    "documentation": "^13.1.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.2",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^23.0.0",
    "memory-fs": "^0.4.1",
    "raw-loader": "^4.0.2",
    "webpack": "^5.11.1"
  },
  "dependencies": {
    "jsdom": "^16.4.0",
    "loader-utils": "^2.0.0"
  },
  "peerDependencies": {
    "webpack": "*",
    "memory-fs": "*"
  }
}
